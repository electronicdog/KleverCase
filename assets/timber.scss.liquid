// ---- SIGNUP POPUP STYLING ---- //



.pop {
	background-color: #eee;
	border: none;
	box-shadow: none;
	width: 100%;
	margin: 0 0 10px 0;
	text-transform: uppercase;
	text-align: center;
}

.awesome {
	width: 100%;
	background-color: #c2502d;
	border: none;
	color: #FFF;
}

.not {
	width: 100%;
	background-color: #eee !important;
	border: none;
	color: #b6b6b6;
	box-shadow: none;
	border: 1px solid #e0e0e0;
}

.not:hover {
	color: #666;
}

.modal {
	width: 60%;
}

.Closebtn {
	border: 2px solid #e0e0e0;
	margin: 0;
	padding: 5px !important;
	text-align: center;
	display: inline-block;
	float: right;
	position: relative;
	color: #e0e0e0;
}

.Closebtn:hover {
	border-color: #c2502d;
	color: #c2502d;
	cursor: pointer;
}

.closebox {
	min-width: 100%;
	display: block;
	overflow: auto;
	position: relative;
}

.poplogo {
	float: left;
	top: 0;
	left: 0;
	width: 220px;
}

fieldset {
	border: none !important;
	margin: 0 !important;
	padding: 0 !important;
}

.modal legend {
	margin: 10px 0;
}

.modal p {
	padding: 10px 0;
	font-family: raleway;
	font-size: 1.4em;
	line-height: 1.25em;
}

.popholder {
	width: 50%;
	margin: 0 auto;
}

.msgholder {
	width: 70%;
	margin: 0 auto;
	text-align: center;
}

input, textarea {
	box-sizing: border-box;
	font-size: 1em;
	line-height: 1em;
	padding: 0.4em;
	height: 3em;
}

input:focus::-webkit-input-placeholder {
	color:transparent;
}

input:focus::-moz-placeholder {
	color:transparent;
}

input:-moz-placeholder {
	color:transparent;
}



@media screen and (-webkit-min-device-pixel-ratio:0) {
	.converter {
		max-height: 2em;
		height: 100%;
		max-width: 77px;
		margin: .25em 0 0 0;
		background: none repeat scroll 0% 0% transparent;
		border: medium none;
		font-family: Raleway;
		color: rgb(194, 80, 45);
		font-weight: bold;
		float: right;
	}
	input {
		border-radius: 0;
	}
}

