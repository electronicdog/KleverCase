// ==============================================================================
// #Typography
// ==============================================================================
body,
input,
textarea,
button,
select {
  font-size: 87.5%; // Henseforth known as 1em (assumed at 14px)
  line-height: 1.6;
  font-family: $bodyFontStack;
  color: $colorText;
  font-weight: 300;
  -webkit-font-smoothing: antialiased;
  -webkit-text-size-adjust: 100%;
}

h1, h2, h3, h4, h5, h6,
.h1, .h2, .h3, .h4, .h5, .h6 {
  font-family: $headerFontStack;
  font-weight: normal;
  text-transform: uppercase;	
  margin-bottom: 10px;
  margin-top: 0.2em;
  margin-bottom: 0.5em;
  line-height: 1.4;

  a {
    text-decoration: none;

    &:hover {
      text-decoration: underline;
    }
  }
}
h1 a, h2 a, h3 a, h4 a, h5 a, h6 a,
.h1 a, .h2 a, .h3 a, .h4 a, .h5 a, .h6 a { font-weight: inherit; }

// Pixel to EM conversion tool http://pxtoem.com/
h1, .h1 { font-size: 2.0em; padding: 2em 0; }       // 28px
h2, .h2 { font-size: 2.429em; }   // 34px
h3, .h3 { font-size: 2em; }       // 28px
h4, .h4 { font-size: 1.429em; }   // 20px
h5, .h5 { font-size: 1.143em; }   // 16px
h6, .h6 { font-size: 1em; }       // 14px

p { margin: 0 0 0.75em 0; }
p img { margin: 0; }

em { font-style: italic; }
strong { font-weight: bold; }
small { font-size: 80%; }

// Blockquotes
blockquote {
  font-size: 1.125em;
  line-height: 1.45;
  color: $colorTextGrey;
  font-style: italic;
  margin: 0 0 $gutter;
  padding: $gutter/2 $gutter;
  border-left: 1px solid $borderColor;

  p {
    margin-bottom: 0;

    & + cite {
      margin-top: $gutter/2;
    }
  }

  cite {
    display: block;
    font-size: 0.75em;
    color: $colorTextGrey;

    &:before {
      content: "\2014 \0020";
    }
  }
}

hr {
  clear: both;
  border-top: solid $borderColor;
  border-width: 1px 0 0;
  margin: $gutter*2 0;

  &.hr-small {
    margin: $gutter 0;
  }

  &.hr-clear {
    border-top: 0 none;
  }
  
  &.notop {
	  margin: 0 0 $gutter 0;
  }
  
  &.nobtm {
	  margin: $gutter 0 0 0;
  }
}



